<AnchorPane fx:id="root" maxHeight="701.0" maxWidth="1001.0" minHeight="699.0" minWidth="999.0" prefHeight="700.0" prefWidth="1000.0" snapToPixel="true" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LeagueIntermissionController">
  <children>
    <VBox alignment="CENTER" layoutX="288.0" layoutY="244.0" prefHeight="388.0" prefWidth="424.0" spacing="40.0">
